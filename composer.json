{
    "name": "pharaonic/laravel-has-files",
    "description": "Laravel - Model's files like Image, Picture, Cover, Thumbnail, Video, Audio, file, ...etc.",
    "keywords": [
        "php",
        "laravel",
        "has files",
        "pharaonic"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Raggi",
            "email": "support@pharaonic.io",
            "homepage": "https://pharaonic.io"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0",
        "pharaonic/laravel-uploader": ">=1.0",
        "pharaonic/laravel-helpers": ">=1.0"
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Files\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Files\\FilesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
