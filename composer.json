{
    "name": "pharaonic/laravel-has-files",
    "description": "Laravel - Model's files like Image, Picture, Cover, Video, Audio, file, ...etc.",
    "keywords": [
        "laravel files",
        "laravel upload",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggigroup@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=10.0",
        "pharaonic/laravel-uploader": "^4.0",
        "pharaonic/laravel-assistant": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Files\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Files\\FilesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
